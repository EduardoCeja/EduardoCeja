function myfunction5 (invertir=""){
    if(!invertir){
        console.warn("No ingresaste un texto");
    }else{
        console.log(`El texto ingresado se invierte: ${invertir.split('').reverse()} `)
    }
}

myfunction5("Hola Mundo ");
myfunction5();

//Respuesta 

//Funcion expresada
const invertirCadena = (invCadena ="")=> 
(!invCadena)
?console.warn("No ingresaste un texto")
:console.info(invCadena.split("").reverse().join(""));

