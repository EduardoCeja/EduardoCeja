//Problema 1: Programa una función que cuente el número de caracteres de una cadena de texto, pe.mifunction("Hola mundo" devolvera 10.

function miFunction(){
    let saludo="Hola mundo";
    document.getElementById("problema1").innerHTML = saludo.length;
}
miFunction();

//Funcion declarada 

function contarCaracteres(cadena =""){
    if(!cadena){
        console.warn("No has ingresado algun texto");
    }else{
        console.info(`El total de caractares de la cadena ${cadena} 
        tiene ${cadena.length} caracteres`);
    }
}
contarCaracteres();
contarCaracteres("Hola mundo");

//Utilizando un arrow function y la condicional del operador ternario 

const contarCaracteres1 = (cadena1 = "") => 
(!cadena1)
?console.warn("No ingresaste ninguna cadena")
:console.info(`El total de caractares de la cadena ${cadena1}tiene ${cadena1.length} caracteres`);
contarCaracteres1();
contarCaracteres1("Hola mundo a todos")

